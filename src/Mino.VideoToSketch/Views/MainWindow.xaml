<Window
    x:Class="Mino.VideoToSketch.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Mino.VideoToSketch"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="Minogram VideoToSketch"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Header="Open Camera" Command="{Binding OpenCamaraCommand}" />
            </MenuItem>
        </Menu>
        <Grid>
            <ui:SymbolIcon Symbol="Fluent24" />
        </Grid>
    </DockPanel>
</Window>
